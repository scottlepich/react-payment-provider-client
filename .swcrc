{
  "$schema": "http://json.schemastore.org/swcrc",
  "jsc": {
    "externalHelpers": true,
    "parser": {
      "decorators": false,
      "dynamicImport": false,
      "syntax": "typescript",
      "tsx": true
    },
    "paths": {
      "~spreedly/*": ["./src/spreedly/*"],
      "~types/*": ["./types/*"]
    }
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "exclude": [".*.test.tsx?$", "types/*"]
}
