{
  "jsc": {
    "externalHelpers": true,
    "parser": {
      "decorators": false,
      "dynamicImport": false,
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment",
        "throwIfNamespace": true,
        "development": false,
        "useBuiltins": false
      },
      "optimizer": {
        "globals": {
          "vars": {
            "__DEBUG__": "true"
          }
        }
      }
    }
  },
  "env": {
    "targets": {
      "chrome": "79"
    }
  }
}
